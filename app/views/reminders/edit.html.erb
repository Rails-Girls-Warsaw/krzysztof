<%= form_for(@reminder) do |form| %>
  <p><%= form.label("title") %></p>
  <p><%= form.text_field("title") %></p>
  <p><%= form.label("do_before") %></p>
  <p><%= form.date_field("do_before") %></p>
  <p><%= form.label("completed") %></p>
  <p><%= form.check_box("completed") %></p>
  <p><%= form.submit %></p>
<% end %>
<p><%= link_to("Back", reminders_path) %></p>